{
  "expo": {
    "name": "Shop Application",
    "description": "This is a test shop application made with tutorial.",
    "slug": "shop-app",
    "privacy": "public",
    "sdkVersion": "39.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f6f6f6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/icon.png",
        "backgroundColor": "#fff"
      }
    }
  }
}
